{
  "name": "ai-character-creator",
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced AI-powered character creator with multi-model support and automated workflows",
  "homepage": "https://huntman1210.github.io/L.N.A.E/",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server/server.js",
    "dev": "concurrently \"npm start\" \"npm run server\"",
    "dev-full": "concurrently \"npm start\" \"npm run server\" \"node automation/orchestrator.js\"",
    "setup": "./setup.sh",
    "setup-stripe": "node scripts/setup-stripe-plans.js",
    "setup-gpu": "./scripts/setup-local-gpu.sh",
    "test-integration": "./scripts/test-integration.sh",
    "test-modes": "node tests/test-mode-system.js",
    "setup-ai": "node automation/setup.js",
    "run-agents": "node automation/agents/orchestrator.js",
    "test-models": "node automation/models/test-all-models.js",
    "generate-website": "node automation/workflows/website-generator.js",
    "generate-app": "node automation/workflows/app-generator.js",
    "generate-game": "node automation/workflows/game-generator.js",
    "social-automation": "node automation/workflows/social-automation.js",
    "demo": "npm run dev",
    "demo:setup": "./setup-demo.sh",
    "demo:reset": "node scripts/setup-demo-data.js",
    "demo:build": "REACT_APP_DEMO_MODE=true npm run build",
    "lint": "eslint . --ext .js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "deploy:demo": "npm run demo:build && echo 'Demo built for deployment'",
    "modes:init": "node scripts/initialize-mode-ecosystem.js",
    "modes:validate": "node scripts/validate-modes.js",
    "modes:demo": "npm start -- --mode-ecosystem",
    "modes:test": "npm test -- --testPathPattern=modes",
    "modes:build": "npm run build && echo \"Mode ecosystem built successfully\""
  },
  "keywords": [
    "react",
    "ai",
    "automation",
    "character-creator",
    "multi-model",
    "autogen",
    "langchain",
    "mode-ecosystem",
    "development-modes",
    "specialized-tools",
    "ai-assisted-development",
    "mode-ecosystem",
    "development-modes",
    "specialized-tools",
    "ai-assisted-development"
  ],
  "author": "AI Automation System",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@google/generative-ai": "^0.12.0",
    "@langchain/anthropic": "^0.2.0",
    "@langchain/core": "^0.2.0",
    "@langchain/google-genai": "^0.0.20",
    "@langchain/mistralai": "^0.0.26",
    "@langchain/ollama": "^0.0.4",
    "@langchain/openai": "^0.2.0",
    "@mistralai/mistralai": "^0.4.0",
    "@react-three/drei": "^9.105.0",
    "@react-three/fiber": "^8.16.0",
    "@sendgrid/mail": "^8.1.5",
    "axios": "^1.7.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "express-rate-limit": "^7.5.0",
    "file-saver": "^2.0.5",
    "formik": "^2.4.0",
    "framer-motion": "^11.18.2",
    "helmet": "^8.1.0",
    "langchain": "^0.2.0",
    "lodash": "^4.17.0",
    "mongoose": "^8.16.0",
    "nodemailer": "^7.0.3",
    "ollama": "^0.5.0",
    "openai": "^4.52.0",
    "playwright": "^1.44.0",
    "puppeteer": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-feather": "^2.0.10",
    "react-image-gallery": "^1.4.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.3",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "stripe": "^18.2.1",
    "styled-components": "^6.1.19",
    "tailwindcss": "^3.4.0",
    "three": "^0.164.0",
    "uuid": "^9.0.0",
    "yup": "^1.4.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.0",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}